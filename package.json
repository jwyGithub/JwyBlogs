{
    "name": "vite-plugin-auto-alias",
    "type": "module",
    "version": "0.2.0",
    "description": "automatically generate alias based on path",
    "author": "jiangweiye <jiangweiye@outlook.com> (https://github.com/jwyGithub)",
    "license": "MIT",
    "homepage": "https://github.com/jwyGithub/vite-plugin-auto-alias",
    "keywords": [
        "vite",
        "vite-plugin",
        "alias"
    ],
    "exports": {
        ".": {
            "require": "./dist/index.cjs.js",
            "import": "./dist/index.esm.js"
        }
    },
    "main": "./dist/index.cjs.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "alias.json"
    ],
    "scripts": {
        "build": "rollup -c rollup.config.ts --configPlugin rollup-plugin-typescript2",
        "release": "npm run build && bumpp package.json --commit --push --tag && npm publish --access=public",
        "lint": "eslint ."
    },
    "peerDependencies": {
        "vite": "*"
    },
    "devDependencies": {
        "@jiangweiye/eslint-config": "^0.0.27",
        "@types/node": "^18.15.3",
        "bumpp": "^9.0.0",
        "eslint": "^8.36.0",
        "rollup": "^3.19.1",
        "rollup-plugin-typescript2": "^0.34.1",
        "typescript": "^5.0.2",
        "vite": "^4.2.0"
    }
}
