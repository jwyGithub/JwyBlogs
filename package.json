{
    "name": "vite-plugin-auto-alias",
    "type": "module",
    "version": "0.2.13",
    "description": "automatically generate alias based on path",
    "author": "jiangweiye <jiangweiye@outlook.com> (https://github.com/jwyGithub)",
    "license": "MIT",
    "homepage": "https://github.com/jwyGithub/vite-plugin-auto-alias",
    "keywords": [
        "vite",
        "vite-plugin",
        "alias"
    ],
    "exports": {
        ".": {
            "require": "./dist/index.cjs.js",
            "import": "./dist/index.esm.js",
            "types": "./dist/types/index.d.ts"
        }
    },
    "main": "./dist/index.cjs.js",
    "module": "./dist/index.esm.js",
    "types": "./dist/types/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rollup -c rollup.config.ts --configPlugin rollup-plugin-typescript2",
        "release": "npm run build && bumpp package.json --commit --push --tag && npm publish --access=public",
        "lint": "eslint ."
    },
    "peerDependencies": {
        "vite": "*"
    },
    "optionalDependencies": {
        "@jiangweiye/tsconfig": "*"
    },
    "devDependencies": {
        "@jiangweiye/eslint-config": "^0.0.30",
        "@jiangweiye/tsconfig": "0.0.6",
        "@rollup/plugin-terser": "^0.4.3",
        "@types/node": "^18.15.3",
        "bumpp": "^9.0.0",
        "eslint": "^8.49.0",
        "rollup": "^3.29.1",
        "rollup-plugin-typescript2": "^0.35.0",
        "typescript": "^4.9.5",
        "vite": "^4.2.0"
    }
}

